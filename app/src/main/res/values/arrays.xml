        <item>@color/light_green</item>
        <item>@color/light_purple</item>
        <item>@color/light_orange</item>
        <item>@color/light_cyan</item>

    </array>

    <array name="icons">
        <item>@drawable/slider</item>
        <item>@drawable/creator</item>
        <item>@drawable/viewer</item>
        <item>@drawable/done</item>

